<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("OCR", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Search Available Positions", "Index", "Position/Index", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Applied Positions", "Index", "Position/Index", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Scheduled Interviews", "Index", "Interview/Index", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Profile", "Index", "User/Index", new { area = "" }, new { @class = "navbar-brand" })










            </div>

            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav"></ul>
            </div>
            @*@Html.ActionLink("Students", "Index", "Student")
            @Html.ActionLink("Events", "Index", "Events")
            @Html.ActionLink("Committees", "Index", "Committees")
            @Html.ActionLink("Admin", "Index", "RoleAdmin")*@
           
            @Html.Partial("_LoginPartial")





        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>



    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>